<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <div class="row">
        <span class="infos-title"><%= park.name %></span>
      </div>
      <div class="row" style="border-bottom: 1px solid #ccc;">
        <span class="infos-addres"><%= park.address %></span>
      </div>
      <div class="row" style="border-bottom: 1px solid #ccc; padding: 8px;">
        <span class="infos-time">HORÁRIO DE FUNCIONAMENTO <span>6:00am - 10:00pm</span></span>
      </div>
      <div class="row" style="margin-top: 5px;">
        <div class="col-sm-4 col-sm-offset-2">VAGAS</div>
        <div class="col-sm-6"><div class="infos-free"><%= park.freeLots %></div></div>
      </div>
      
      <div class="row text-center" style="margin-top: 5px; background: #eee;">
        <span class="row">CONVÊNIOS</span>
        <span class=""><%= park.shops %></span>
      </div>
    </div>
  </div>
</div>