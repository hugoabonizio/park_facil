<form action="/park/salvarTicket" method="post">
  <div class="row">
    <div class="col-lg-3">
      <label>Placa do Veículo</label>
      <input type="text" class="form-control" placeholder="AAA-9999" name="placa">
    </div>

    <div class="col-lg-3">
      <label>Telefone</label>
      <input type="text" class="form-control" placeholder="(XX) 99999-9999" name="telefone">
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-lg-3">
      <label class="control-label">Tipo de Veículo</label>
      <label class="radio">
        <input type="radio" name="tipoVeiculo" value="Automóvel" checked>Automóvel
      </label>
      <label class="radio">
        <input type="radio" name="tipoVeiculo" value="Motocicleta">Motocicleta
      </label>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-lg-offset-4 col-lg-4">
      <input type="submit" class="form-control">
    </div>
  </div>
</form>
